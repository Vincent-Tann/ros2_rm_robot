          <remapping>~/camera_info:=segmentation_camera_info</remapping>
        </ros>
        <camera_name>segmentation_camera</camera_name>
        <frame_name>segmentation_camera_link</frame_name>
      </plugin>
    </sensor>
  </gazebo>
  <joint name="segmentation_camera_joint" type="fixed">
